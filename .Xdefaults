URxvt.termName:     rxvt-256color
URxvt.font:         xft:FuraCode Nerd Font Mono:style=Regular:size=14
URxvt.scrollstyle:  plain
URxvt.scrollBar:    false
URxvt.depth:        32
!Use URxvt.tintColor
URxvt.transparent:  true
!Set transparency amount if transparency is on
URxvt.shading:      25 
!Amount to fade inactive terminal window
URxvt.fading:       10

!*.foreground:   #75cc60
*.foreground:   #b8bb26
*.background:   #282828

! Gruvbox dark theme with custom color9
! black
*.color0:       #282828
*.color8:       #928374

! red
*.color1:       #cc241d
*.color9:       #c31dcc

! green
*.color2:       #98971a
*.color10:      #b8bb26

! yellow
*.color3:       #d79921
*.color11:      #fabd2f

! blue
*.color4:       #458588
*.color12:      #83a598

! purple
*.color5:       #b16286
*.color13:      #d3869b

! aqua
*.color6:       #689d6a
*.color14:      #8ec07c

! white
*.color7:       #a89984
*.color15:      #ebdbb2
